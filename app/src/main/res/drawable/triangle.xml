<?xml version="1.0" encoding="utf-8"?>
<vector selector xmlns:android="http://schemas.android.com/apk/res/android"
    android:height="80dp"
    android:width="100dp"
    android:viewportHeight="100"
    android:viewportWidth="100">

    <group android:name="triangle">
        <path
            android:name="triangle"
            android:fillColor="@color/black"
            android:pathData="m 50,0 l 50,100 -100 z"
            android:strokeColor="@color/white"
            android:strokeWidth="2"/>
    </group>

</vector>